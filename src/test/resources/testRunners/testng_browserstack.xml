<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="BrowserStack Test automtion regression suite"
	thread-count="3" parallel="tests" verbose="3">


	
	<test name="BrowserStack Test - FF">
		<parameter name="os" value="Windows" />
		<parameter name="osVersion" value="10" />
		<parameter name="browserName" value="Firefox" />
		<parameter name="browserVersion" value="102.0" />
		
		
		

		<classes>
			<class name="com.tests.BrowserStackTest" />
		</classes>

	</test>

	<test name="BrowserStack Test - Chrome">
		<parameter name="os" value="OS X" />
		<parameter name="osVersion" value="Catalina" />
		<parameter name="browserName" value="Chrome" />
		<parameter name="browserVersion" value="110.0" />

		<classes>
			<class name="com.tests.BrowserStackTest" />
		</classes>

	</test>










</suite>